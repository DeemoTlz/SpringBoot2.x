<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.albrus.shiro.mapper.RoleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.albrus.shiro.entity.Role">
    <result column="id" property="id" />
    <result column="generate_by" property="generateBy" />
    <result column="generate_time" property="generateTime" />
    <result column="update_by" property="updateBy" />
    <result column="update_time" property="updateTime" />
        <result column="role_name" property="roleName" />
        <result column="description" property="description" />
        <result column="predefined" property="predefined" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        generate_by,
        generate_time,
        update_by,
        update_time,
        role_name, description, predefined, deleted
    </sql>

</mapper>
